{
  "name": "reactjs-starter",
  "version": "1.0.1",
  "description": "ReactJS Starter",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "rm -rf ./.build && npm-run-all --parallel dev:client dev:server",
    "dev:client": "webpack-dev-server --define process.env.NODE_ENV='\"development\"' --config webpackconfig/webpack.config.devClient.js",
    "dev:server": "webpack --define process.env.NODE_ENV='\"development\"' --config webpackconfig/webpack.config.devServer.js",
    "hml": "rm -rf ./.build && npm-run-all --parallel hml:client hml:server",
    "hml:client": "webpack --define process.env.NODE_ENV='\"homologation\"' --config webpackconfig/webpack.config.prodClient.js --optimize-minimize",
    "hml:server": "webpack --define process.env.NODE_ENV='\"homologation\"' --config webpackconfig/webpack.config.prodServer.js --optimize-minimize",
    "azul": "rm -rf ./.build && npm-run-all --parallel azul:client azul:server",
    "azul:client": "webpack --define process.env.NODE_ENV='\"blue\"' --config webpackconfig/webpack.config.prodClient.js --optimize-minimize",
    "azul:server": "webpack --define process.env.NODE_ENV='\"blue\"' --config webpackconfig/webpack.config.prodServer.js --optimize-minimize",
    "prd": "rm -rf ./.build && npm-run-all --parallel prd:client prd:server",
    "prd:client": "webpack --define process.env.NODE_ENV='\"production\"' --config webpackconfig/webpack.config.prodClient.js -p",
    "prd:server": "webpack --define process.env.NODE_ENV='\"production\"' --config webpackconfig/webpack.config.prodServer.js -p",
    "build": "node ./.build/app.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "file-loader": "^1.1.5",
    "start-server-webpack-plugin": "^2.1.2",
    "style-loader": "^0.19.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.1",
    "css-loader": "^0.25.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "history": "^4.7.2",
    "html-minifier": "^3.5.5",
    "immutee": "^1.0.1",
    "node-fs-extra": "^0.8.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.7",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^4.4.8",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "redux-universal": "0.0.2",
    "sass-loader": "^4.1.1",
    "serve-favicon": "^2.4.5",
    "webpack": "^2.7.0",
    "window-or-global": "^1.0.1"
  }
}
